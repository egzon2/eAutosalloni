@model Brand

<div class="bg-contact2" style="background-image: url('~/wwwroot/AdminTheme/ContactFrom_v2/images/bg-01.jpg');">
		<div class="container-contact2">
			<div class="wrap-contact2">
                <form class="contact2-form validate-form" asp-action="Create" method="post" enctype="multipart/form-data">
                    <span class="contact2-form-title">
                        Create Brand
                    </span>
                    <div asp-validation-summary="All" class="text-danger"></div>
                    <div class="wrap-input2 validate-input" data-validate="Name is required">
                        <input asp-for="@Model.Name" class="input2" placeholder="Model">
                        <span class="focus-input2" data-placeholder="Model Name"></span>
                    </div>

                    <div class="wrap-input2 validate-input">
                        <input asp-for="@Model.Country" class="input2" placeholder="Country">
                        <span class="focus-input2" data-placeholder="Country"></span>
                    </div>               

                    <div class="container-contact2-form-btn">
                        <div class="wrap-contact2-form-btn">
                            <div class="contact2-form-bgbtn"></div>
                            <button class="contact2-form-btn">
                                Create
                            </button>
                        </div>
                    </div>
                </form>
			</div>
		</div>
	</div>
